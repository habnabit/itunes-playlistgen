{
    "defaultSeverity": "error",
    "extends": [],
    "jsRules": {},
    "rules": {
        "semicolon": false,
        "quotemark": false,
        "prefer-const": true,
        "trailing-comma": [
            true,
            {
                "multiline": {
                    "objects": "always",
                    "arrays": "always"
                }
            }
        ],
        "max-classes-per-file": false,
        "arrow-parens": true,
        "ordered-imports": true,
        "import-spacing": true,
        "no-unused-expression": true
    },
    "rulesDirectory": []
}
