        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "keyofStringsOnly": true,
        "strictFunctionTypes": true,
        "alwaysStrict": true
    },
    "include": [
        "./app/**/*.ts",
        "./app/**/*.tsx"
    ]
}
